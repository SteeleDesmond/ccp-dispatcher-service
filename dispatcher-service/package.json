{
  "name": "vmware-install-agent-api",
  "version": "2.5.0",
  "org": "CSPLab",
  "description": "Install Agent API for CSPLab",
  "main": "index.js",
  "config": {
    "protocol": "http",
    "host": "localhost",
    "port": 3005,
    "contextRoot": "",
    "redisHost": "localhost",
    "redisPort": 6379,
    "redisPass": "TempPass",
    "redisNamespace": "rsmq",
    "dispatcherQueue": "dispatcherService",
    "mongoHost": "mongodb://127.0.0.1:27017/vmware-install-agent"
  },
  "pact": {
    "providerBaseUrl": "",
    "pactBrokerUrl": ""
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest --coverage",
    "tdd": "jest --watch",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "npm run swagger",
    "swagger": "swaggerGen -c ./swagger.config.json",
    "prestart": "npm run build",
    "start": "npm run serve",
    "serve": "node dist/src",
    "start:dev": "nodemon ./src/index.ts | ./node_modules/.bin/pino-pretty -c -l",
    "upgrade-interactive": "npm-check --update",
    "pact:verify": "ts-node test/pact-verify",
    "pact:publish": "ts-node test/pact-publish",
    "sonarqube:scan": "npm run test && ts-node test/sonarqube-scan"
  },
  "author": "Steele Desmond - steele.desmond@ibm.com",
  "dependencies": {
    "ajv": "^6.12.5",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "express-pino-logger": "^5.0.0",
    "i": "^0.3.6",
    "lodash": "^4.17.20",
    "maskdata": "^1.1.2",
    "mongoose": "^5.10.6",
    "pino": "^6.6.1",
    "pino-noir": "^2.2.1",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "rsmq": "^0.12.2",
    "rsmq-worker": "^0.5.2",
    "rxjs": "^6.6.3",
    "sonic-boom": "^1.3.0",
    "superagent": "^6.1.0",
    "tslib": "^2.0.1",
    "typescript-ioc": "^3.2.1",
    "typescript-rest": "^3.0.2",
    "typescript-rest-ioc": "^1.0.0",
    "typescript-rest-swagger": "^1.1.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@pact-foundation/pact": "^9.12.0",
    "@pact-foundation/pact-node": "^10.10.1",
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-pino-logger": "^4.0.2",
    "@types/jest": "^26.0.14",
    "@types/jest-plugin-context": "^2.9.2",
    "@types/node": "^14.11.2",
    "@types/pino": "^6.3.1",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.10",
    "@types/yargs": "^15.0.6",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "jest-plugin-context": "^2.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "npm-check": "^5.9.0",
    "pino-pretty": "^4.2.1",
    "rimraf": "^3.0.2",
    "sonarqube-scanner": "^2.7.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "yargs": "^15.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/dte2-0/ccp-vmware-install-agent.git"
  }
}
